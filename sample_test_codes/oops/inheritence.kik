// Inheritance Example

import "io.kik";

class Person {
    public:
        string name;
        int age;

        void showInfo() {
            output(name);
            output(age);
        }
}

class Student : Person {
    public:
        int roll;

        void setRoll(int r) {
            roll = r;
        }

        void showAll() {
            showInfo();
            output(roll);
        }
}

int main() {
    Student s = new Student();
    s.name = "Akash";
    s.age = 20;
    s.setRoll(101);
    s.showAll();

    return 0;
}
